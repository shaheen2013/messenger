{
    "name": "shahinur/messenger",
    "description": "This is a messenger package for laravel 8.x. This package enables developers to add messaging funtionalities to their Laravel applications.",
    "keywords": ["laravel", "messenger", "messages"],
    "license": "MIT",
    "authors": [
        {
            "name": "Shahinur Rahman",
            "email": "shaheenmediusware@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.4|^8.0.2",
        "illuminate/support": "^9.22.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10",
        "orchestra/testbench": "^7.6.0"
    },
    "autoload": {
        "psr-4": {
            "Shahinur\\Messenger\\Tests\\": "tests",
            "Shahinur\\Messenger\\": "src/Messenger/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Shahinur\\Messenger\\MessengerServiceProvider"
            ],
            "aliases": {
                "Messenger": "Shahinur\\Messenger\\Facades\\Messenger"
            }
        }
    }
}
